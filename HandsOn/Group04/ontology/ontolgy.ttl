@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://www.smart-city.gp04-ldata.org/city/ontology/Road-traffic-accidents>
  a owl:Ontology ;
  owl:imports <http://www.smart-city.gp04-ldata.org/> .

<http://www.smart-city.gp04-ldata.org/accidentOccuredAt> rdfs:domain <http://www.smart-city.gp04-ldata.org/RoadTrafficAccidents> .
<http://www.smart-city.gp04-ldata.org/accidentOccuredIn>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/RoadTrafficAccidents> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/accidentDate> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.gp04-ldata.org/atRoadClass>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Road> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/roadClass> ;
    owl:someValuesFrom rdfs:Literal
  ] .

<http://www.smart-city.gp04-ldata.org/hasAgeCasualy>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Casualty> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/age> ;
    owl:someValuesFrom xsd:int
  ] .

<http://www.smart-city.gp04-ldata.org/hasCasualtySeverity>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Casualty> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/casualtySeverity> ;
    owl:someValuesFrom rdf:XMLLiteral
  ] .

<http://www.smart-city.gp04-ldata.org/hasCausualtyClass>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Casualty> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/casualtyClass> ;
    owl:someValuesFrom rdf:XMLLiteral
  ] .

<http://www.smart-city.gp04-ldata.org/hasLightingCondition>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/RoadTrafficAccidents> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/lightCondition> ;
    owl:someValuesFrom rdf:XMLLiteral
  ] .

<http://www.smart-city.gp04-ldata.org/hasNumberOfVehicles>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Vehicle> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/numberVehcile> ;
    owl:someValuesFrom xsd:int
  ] .

<http://www.smart-city.gp04-ldata.org/hasRefNumber>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/RoadTrafficAccidents> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/referenceNumber> ;
    owl:someValuesFrom rdfs:Literal
  ] .

<http://www.smart-city.gp04-ldata.org/hasRoadSurface>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Road> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/roadSurface> ;
    owl:someValuesFrom rdfs:Literal
  ] .

<http://www.smart-city.gp04-ldata.org/hasSexCasualty>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Casualty> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/casualtySex> ;
