  ] .

<http://www.smart-city.gp04-ldata.org/hasTypeOfVehicle>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/Vehicle> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/typeVehicle> ;
    owl:someValuesFrom rdfs:Literal
  ] .

<http://www.smart-city.gp04-ldata.org/hasWeatherCondition>
  rdfs:domain <http://www.smart-city.gp04-ldata.org/RoadTrafficAccidents> ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.gp04-ldata.org/weatherCondition> ;
    owl:someValuesFrom rdfs:Literal
  ] .
