@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.smart-city.GP-LDATA.org/> a owl:Ontology .
<http://www.smart-city.GP-LDATA.org/accidentOccuredAt>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.GP-LDATA.org/accidentTime> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.GP-LDATA.org/accidentOccuredIn>
  a owl:ObjectProperty ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.GP-LDATA.org/accidentDate> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.GP-LDATA.org/atRoadClass>
  a owl:ObjectProperty ;
  rdfs:domain [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.GP-LDATA.org/roadClass> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.GP-LDATA.org/hasAgeCasualy>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.GP-LDATA.org/age> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.GP-LDATA.org/hasCasualtySeverity>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:range [
    a owl:Restriction ;
    owl:onProperty <http://www.smart-city.GP-LDATA.org/casualtySeverity> ;
    owl:someValuesFrom owl:rational
  ] .

<http://www.smart-city.GP-LDATA.org/hasCausualtyClass>
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty .

<http://www.smart-city.GP-LDATA.org/hasEasting>
