@prefix acc: <http://purl.org/linked-data/accidents-in-Madrid/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace>
@prefix dc: <http://purl.org/dc/elements/1.1/>
@prefix ra: <ra:http://www.w3.org/2012/7/ra3.owl#>

#Classes

:Thing          rdf:type owl:Class .
:LivingBeing    rdf:type owl:Class ;
                owl:propertyDisjointWith : NonLivingThing .
:NonLivingThing rdf:type owl:Class ;
                owl:propertyDisjointWith : LivingThing .
:Event          rdf:type owl:Class .
:Location       rdf:type owl:Class .
:Document       rdf:type owl:Class .
:Person         rdf:type owl:Class .
:Vehicle        rdf:type owl:Class .
:Enviroment     rdf:type owl:Class .

#ObjectProperties

:isA              rdf:type owl:ObjectProperty .
:itWasIn          rdf:type owl:ObjectProperty .
:has              rdf:type owl:ObjectProperty .
:suffered         rdf:type owl:ObjectProperty .
:happened         rdf:type owl:ObjectProperty .
:isPartOf         rdf:type owl:ObjectProperty .
:ageRange         rdf:type owl:DataProperty .
:vehicleCategory  rdf:type owl:DataProperty .
:accidentType     rdf:type owl:DataProperty .
:weatherCondition rdf:type owl:DataProperty .
:surfaceCondition rdf:type owl:DataProperty .
:relatedInjury    rdf:type owl:DataProperty .
:address          rdf:type owl:DataProperty .
:date             rdf:type owl:DataProperty .
:parteID          rdf:type owl:DataProperty .
:numberOfVictims  rdf:type owl:DataProperty .

#Domain and ranges

:isA              rdfs:domain :Thing ;
                  rdfs:range  :Thing .
:itWasIn          rdfs:domain :Thing ;
                  rdfs:range  :Location .
:has              rdfs:domain :Person ;
                  rdfs:range  :Age .
:suffered         rdfs:domain :Person ;
                  rdfs:range  :Injury .
:happened         rdfs:domain :Thing ;
                  rdfs:range  :Date .
:isPartOf         rdfs:domain :Thing ;
                  rdfs:range  :Thing .
:ageRange         rdfs:domain :Person ;
                  rdfs:range  xsd:integer .
:vehicleCategory  rdfs:domain :Vehicle ;
                  rdfs:range  {"bicycle" , "bus" , "car" ,
                  "motocycle" , "truck"} .
:accidentType     rdfs:domain :Event ;
                  rdfs:range  {"fall" , "crush"} .
:weatherCondition rdfs:domain :Enviroment ;
                  rdfs:range  {"rain" , "snow" , "hail"} .
:surfaceCondition rdfs:domain :Enviroment ;
                  rdfs:range  {"wet" , "dry" , "oil"} .
:relatedInjury    rdfs:domain :Person ;
                  rdfs: range {"fatal" , "light" , "none" , "severe"} .
:address          rdfs:domain :Location ;
                  rdfs:range  xsd:string .
:date             rdfs:domain :NonLivingThing ;
                  rdfs:range  xsd:date .
:parteID          rdfs:domain :Document ;
                  rdfs:range  xsd:string .
:numberOfVictims  rdf:domain  :Event ;
                  rdfs:range  xsd:integer.
